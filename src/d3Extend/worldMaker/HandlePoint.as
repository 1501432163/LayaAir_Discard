package laya.d3Extend.worldMaker {
	public class HandlePoint {
		public var x:Number
		public var y:Number;
		public var ctrlx:Number;		// 控制点
		public var ctrly:Number;		// 控制点
		public function HandlePoint(_x:Number, _y:Number, _ctrlx:Number, _ctrly:Number):void {
			x = _x; y = _y; ctrlx = _ctrlx; ctrly = _ctrly;
		}
	}
}	
	